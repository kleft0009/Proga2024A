// Clase semana N10
#include <iostream>
using namespace std;
//programa para sumar dos numeros
int main() {
    int a, b,c;
    a=3;
    cout<<"Suma de numeros\n";
    cout<<"Ingrese el 1er numero: ";
    cin>>a;
    cout<<"Ingrese el 2do numero: ";
    cin>>b;
    c=a+b;
    cout<<"Resultado de"<<c;
    return 0;
}
