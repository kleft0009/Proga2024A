#include <iostream>
using namespace std;
int main() {
    //me permite menores a 4 tambien
    int numero[4]={1,10,3,4};
    int media;
    numero[0]=1;
    numero[1]=1;
    numero[2]=1;
    numero[3]=1;
    media=(numero[0]+numero[1]+numero[2]+numero[3]);
    cout<<"Media es: "<<media<<end;
    cout<<numero[3];
    return 0;
}
